                <SetGlobalStep id="_1p6Z" local="true" varname="result">
                  <default>true</default>
                </SetGlobalStep>
              </ElseifSequence>
              <ElseifSequence id="_1p6a" test="'$(purpose)' == 'BndCal'">
                <SetGlobalStep id="_1p6b" local="true" varname="result">
                  <default>true</default>
                </SetGlobalStep>
              </ElseifSequence>
              <ElseifSequence id="_1p6c" test="'$(purpose)' == 'PhsCal'">
                <SetGlobalStep id="_1p6d" local="true" varname="result">
                  <default>true</default>
                </SetGlobalStep>
