              <ProcedureCall id="_1obx" local="true"
                             procedure="almaot.project.getScienceGoalSpectralSetupContinuumSkyFreq"
                             retvarname="skyFreqRef">
                <variable name="type">$(type)</variable>
                <variable name="name">$(name)</variable>
              </ProcedureCall>
              <SetGlobalStep id="_1oez" local="true" varname="numSWRef">
                <default>4</default>
              </SetGlobalStep>
              <ElseSequence id="_1ocj">
                <MouseEventStep clicks="1" client="$(client)"
