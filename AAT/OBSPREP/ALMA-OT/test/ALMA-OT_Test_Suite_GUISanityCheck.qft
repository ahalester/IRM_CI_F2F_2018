          <Procedure id="_1lva" name="getNewSpectralWindowName">
            <variable name="baseBandName"/>
            <comment>The name for a new spectral window is determined by counting the existing ones.

@param	baseBandName		Name of base band to contain the new spectral window.

@author	Peter Kratzer
</comment>
            <SetGlobalStep id="_1lvb" local="true" varname="result"/>
            <SetGlobalStep id="_1lvc" local="true" varname="count">
              <default>1</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1lvd" local="true" varname="continue">
              <default>true</default>
            </SetGlobalStep>
            <WhileSequence id="_1lve" test="$(count) &lt; 9 and $(continue)">
              <comment>Max. 8 BBs available, but not for all receiver bands.
No check is done whether this maximum is available for the selected receiver band.
</comment>
              <TryStep id="_1lvf" name="Access group node if existent">
                <SetGlobalStep id="_1lvg" local="true" varname="SWName">
                  <default>SW-$(count)</default>
                  <comment>
</comment>
                </SetGlobalStep>
                <TryStep id="_1lvh"
                         name="Check if SW with specified name is already listed in table">
                  <MouseEventStep clicks="1" client="$(client)"
                                  component="tab$(baseBandName).tabbed.$(SWName)"
                                  event="MOUSE_MPRC" id="_1lw8" modifiers="16"
                                  x="1" y="1"/>
                  <ProcedureCall id="_1lwB"
                                 procedure="qfs.swing.tabbedpane.selectTab">
                    <variable name="id">tab$(baseBandName).tabbed</variable>
                    <variable name="separator">@</variable>
                    <variable name="value">$(SWName)</variable>
                  </ProcedureCall>
                  <SetGlobalStep id="_1lvj" local="true" varname="count">
                    <default>$[$(count) + 1]</default>
                    <comment>If previous operation succeeded a BB with the specified name exists already.</comment>
                  </SetGlobalStep>
                  <CatchSequence exception="IndexNotFoundException" id="_1lvk"
                                 maxerror="0">
                    <comment>This exception occurs when at least one baseband is already defined but the specified name is not found.</comment>
                    <SetGlobalStep id="_1lvl" local="true" varname="result">
                      <default>$(SWName)</default>
                    </SetGlobalStep>
                    <SetGlobalStep id="_1lvm" local="true" varname="continue">
                      <default>false</default>
                    </SetGlobalStep>
                  </CatchSequence>
                  <CatchSequence exception="ComponentNotFoundException"
                                 id="_1lvn" maxerror="0">
                    <comment>This exception occurs when at no baseband is defined yet (so no more name check is required).</comment>
                    <SetGlobalStep id="_1lvo" local="true" varname="result">
                      <default>$(SWName)</default>
                    </SetGlobalStep>
                    <SetGlobalStep id="_1lvp" local="true" varname="continue">
                      <default>false</default>
                    </SetGlobalStep>
                  </CatchSequence>
                </TryStep>
              </TryStep>
            </WhileSequence>
            <ReturnStep id="_1lvz" retval="$(result)"/>
          </Procedure>
