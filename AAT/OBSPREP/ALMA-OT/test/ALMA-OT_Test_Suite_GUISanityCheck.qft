value2 = rc.lookup("value2")
decimals = rc.lookup("decimals")
if $(fluxUnitFlag) == 1:
    decimals = 1

check = value1.strip().split(" ", 1)
if len(check) &gt; 1:
    value1 = float(check[0].strip())
    unit1 = check[1].strip()
    if unit1 == 'fomb':
        unit1 = 'fraction of main beam'
else:
    value1 = float(value1)
    unit1 = ''

check = value2.strip().split(" ", 1)
if len(check) &gt; 1:
    value2 = float(check[0].strip())
    unit2 = check[1].strip()
    if unit2 == 'fomb':
        unit2 = 'fraction of main beam'
else:
    value2 = float(value2)
    unit2 = ''

#print "fluxUnitFlag is : ", fluxUnitFlag
#print '#1 (%s,%s)' % (value1, unit1)
#print '#2 (%s,%s)' % (value2, unit2)

# For ALMA108: the piece of code below has been duplicated 
# for mJy as well
if unit1 == 'Jy' and unit2 == 'kJy':
    unit1 = 'kJy'
    value1 = value1 / 1000.0
elif unit1 == 'kJy' and unit2 == 'Jy':
    unit2 = 'kJy'
    value2 = value2 / 1000.0
elif unit1 == 'Jy' and unit2 == 'mJy':
    unit1 = 'kJy'
    value1 = value1 / 1000.0
    unit2 = 'kJy'
    value2 = value2 / 1000000.0
elif unit1 == 'mJy' and unit2 == 'Jy':
    unit1 = 'kJy'
    unit2 = 'kJy'
    value1 = value1 / 1000000.0
    value2 = value2 / 1000.0
elif unit1 == 'mJy' and unit2 == 'kJy':
    unit1 = 'kJy'
    value1 = value1 / 1000000.0
elif unit1 == 'kJy' and unit2 == 'mJy':
    unit1 = 'kJy'
    unit2 = 'kJy'
    value2 = value2 / 1000000.0

if unit1 == unit2:
    epsilon = pow(10, int(decimals))
    #print 'Epsilon = ', epsilon
    diff = abs(value1 - value2)
    #print "Diff: ", diff
    if diff &lt; epsilon:
        result = True
#    else:
#        print 'Diff(%s,%s) = %f', (value1, value2, diff)
#else:
#    print "(", unit1, "&lt;&gt;", unit2, ")" 

# Hack for "fraction of main beam"
#else:
#    #print 'unit1 &lt;&gt; unit2', " (", unit1, "&lt;&gt;", unit2, ")" 
#    if unit1 == 'fraction of main beam' or unit2 == 'fraction of main beam':
#        result = True

#print 'Result: ', result

rc.setLocal("result", result)</code>
          </ServerScriptStep>
          <ReturnStep id="_1ohp" retval="$(result)"/>
        </Procedure>
