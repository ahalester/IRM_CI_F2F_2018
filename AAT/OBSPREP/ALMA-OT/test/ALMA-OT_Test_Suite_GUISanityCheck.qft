        <Procedure id="_1oby" name="getAlmaBandForFreq">
          <variable name="freqVal"/>
          <variable name="freqUnit">GHz</variable>
          <comment>The ALMA band for the specified frequency is determined.

@param	freqVal	Numeric part of the evaluated frequency.
@param	freqUnit	Unit of the evaluated frequency.

@return	Determined ALMA band, integer value within the range [1, 10]. If an invalid frequency (i.e. not supported by the ALMA receivers) is specified the value &lt;strong&gt;-1&lt;/strong&gt; is returned.

@author	Peter Kratzer
</comment>
          <SetGlobalStep id="_1oc4" local="true" varname="result">
            <default>-1</default>
          </SetGlobalStep>
          <IfSequence id="_1obz" test="'$(freqUnit)' == 'MHz'">
            <SetGlobalStep id="_1ob+" local="true" varname="freqVal">
              <default>$[$(freqVal)  / 1000]</default>
            </SetGlobalStep>
            <ElseifSequence id="_1ob-" test="'$(freqUnit)' == 'kHz'">
              <SetGlobalStep id="_1oc0" local="true" varname="freqVal">
                <default>$[$(freqVal)  / 1000000]</default>
              </SetGlobalStep>
            </ElseifSequence>
            <ElseifSequence id="_1oc1" test="'$(freqUnit)' == 'Hz'">
              <SetGlobalStep id="_1oc2" local="true" varname="freqVal">
                <default>$[$(freqVal)  / 1000000000]</default>
              </SetGlobalStep>
            </ElseifSequence>
          </IfSequence>
          <IfSequence id="_1oc3" test="$(freqVal) &lt; 31.3">
            <ProcedureCall id="_1oc5" procedure="qfs.run-log.logError">
              <variable name="message">Frequence $(freqVal) GHz is not observable with ALMA.</variable>
              <variable name="withScreenshots">false</variable>
            </ProcedureCall>
            <ElseifSequence id="_1oc6" test="$(freqVal) &lt; 67">
              <SetGlobalStep id="_1oc7" local="true" varname="result">
                <default>1</default>
              </SetGlobalStep>
            </ElseifSequence>
            <ElseifSequence id="_1oc8" test="$(freqVal) &lt; 84">
              <SetGlobalStep id="_1oc9" local="true" varname="result">
                <default>2</default>
              </SetGlobalStep>
            </ElseifSequence>
            <ElseifSequence id="_1ocA" test="$(freqVal) &lt; 125">
              <SetGlobalStep id="_1ocB" local="true" varname="result">
                <default>3</default>
              </SetGlobalStep>
            </ElseifSequence>
            <ElseifSequence id="_1ocC" test="$(freqVal) &lt; 163">
              <SetGlobalStep id="_1ocD" local="true" varname="result">
                <default>4</default>
              </SetGlobalStep>
            </ElseifSequence>
            <ElseifSequence id="_1ocE" test="$(freqVal) &lt; 211">
