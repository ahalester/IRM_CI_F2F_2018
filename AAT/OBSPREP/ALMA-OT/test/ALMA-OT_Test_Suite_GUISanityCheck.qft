                           procedure="qfs.swing.tree.selectNode">
              <variable name="id">$(tab).tree</variable>
              <variable name="node">$(path)/$(name)/Field Setup</variable>
              <variable name="separator">%</variable>
            </ProcedureCall>
            <ProcedureCall id="_1qk9"
                           procedure="almaot.project.selectScienceGoalFieldSetup">
              <variable name="type">$(type)</variable>
              <variable name="name">$(name)</variable>
            </ProcedureCall>
            <ProcedureCall id="_1oa6" local="true"
                           procedure="almaot.project.getScienceGoalNumSources"
                           retvarname="numSourcesRef">
              <variable name="type">$(type)</variable>
              <variable name="name">$(name)</variable>
            </ProcedureCall>
            <ProcedureCall disabled="true" id="_1ocb"
                           procedure="qfs.swing.tree.selectNode">
              <variable name="id">$(tab).tree</variable>
              <variable name="node">$(path)/$(name)/Spectral Setup</variable>
              <variable name="separator">%</variable>
            </ProcedureCall>
            <ProcedureCall id="_1qkj"
                           procedure="almaot.project.selectScienceGoalSpectralSetup">
              <variable name="type">$(type)</variable>
              <variable name="name">$(name)</variable>
              <variable name="calibrator">false</variable>
            </ProcedureCall>
            <ProcedureCall id="_1s4I" local="true"
                           procedure="almaot.project.getScienceGoalSpectralSetupContinuumRepFreq"
                           retvarname="repFreqRef">
              <variable name="type">$(type)</variable>
              <variable name="name">$(name)</variable>
              <variable name="standardise">true</variable>
            </ProcedureCall>
            <ProcedureCall id="_1s4z" local="true"
                           procedure="almaot.project.getScienceGoalSpectralSetupContinuumRepSkyFreq"
                           retvarname="skyFreqRef">
              <variable name="type">$(type)</variable>
              <variable name="name">$(name)</variable>
              <variable name="index">0</variable>
              <variable name="standardise">true</variable>
            </ProcedureCall>
            <CheckBooleanStep checktype="selected" client="$(client)"
                              component="SpectralType.radioSingleContinuum"
                              id="_1och" local="true" resvarname="continuum"/>
            <IfSequence id="_1s4V" test="$(continuum)">
              <SetGlobalStep id="_1s4W" local="true" varname="numSWRef">
                <default>4</default>
              </SetGlobalStep>
              <ElseSequence id="_1s4+">
                <ProcedureCall id="_1s4-" local="true"
                               procedure="almaot.project.getScienceGoalSpectralSetupNumSW"
                               retvarname="numSWRef">
                  <variable name="type">$(type)</variable>
                  <variable name="name">$(name)</variable>
                </ProcedureCall>
              </ElseSequence>
            </IfSequence>
