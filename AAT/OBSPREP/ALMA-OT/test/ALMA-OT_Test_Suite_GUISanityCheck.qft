        <Procedure id="_1og0" name="getScienceGoalCtrlPerfSensitivity">
          <variable name="type"/>
          <variable name="name"/>
          <comment>Return normalised sensitivity setting for selected science goal.

@param	type	Project type. One of the values (Proposal, Program).
@param	name	Science goal name.

@return	Sensitivity normalised to Jy or K (string) with numeric value and unit separated by blank.

@author	Peter Kratzer</comment>
          <ProcedureCall id="_1qkv"
                         procedure="almaot.project.selectScienceGoalCtrlPerf">
            <variable name="type">$(type)</variable>
            <variable name="name">$(name)</variable>
          </ProcedureCall>
          <SetGlobalStep id="_1og4" local="true" varname="result"/>
          <CheckBooleanStep checktype="visible" client="$(client)"
                            component="SfiControl.sensitivity.value"
                            disabled="true" id="_1ogB"/>
          <CheckBooleanStep checktype="visible" client="$(client)"
                            component="SfiControl.sensitivity.unit"
                            disabled="true" id="_1ogC"/>
          <CheckItemsStep client="$(client)"
                          component="SfiControl.sensitivity.unit"
                          disabled="true" id="_1ogD">
            <item>Jy</item>
            <item>mJy</item>
            <item>K</item>
            <item>mK</item>
            <item>uJy</item>
          </CheckItemsStep>
          <CheckBooleanStep checktype="visible" client="$(client)"
                            component="SfiControl.sensitivityFB.unit"
                            disabled="true" id="_1ogE"/>
          <CheckItemsStep client="$(client)"
                          component="SfiControl.sensitivityFB.unit"
                          disabled="true" id="_1ogF">
            <item>mK</item>
            <item>K</item>
          </CheckItemsStep>
          <FetchTextStep client="$(client)"
                         component="SfiControl.sensitivity.value" id="_1og7"
                         local="true" varname="sensitivity"/>
          <FetchTextStep client="$(client)"
                         component="SfiControl.sensitivity.unit" id="_1og8"
                         local="true" varname="sensitivityUnit"/>
          <IfSequence disabled="true" id="_1ogM"
                      test="'$(sensitivityUnit)' == 'K'">
            <FetchTextStep client="$(client)"
                           component="SfiControlParameterEditor.EquivalentToText"
                           id="_1ogN" local="true" varname="sensitivity"/>
            <FetchTextStep client="$(client)"
                           component="SfiControl.sensitivityFB.unit"
                           id="_1ogc" local="true" varname="sensitivityUnit"/>
          </IfSequence>
          <IfSequence disabled="true" id="_1ogd"
                      test="'$(sensitivityUnit)' == 'mK'">
            <FetchTextStep client="$(client)"
                           component="SfiControlParameterEditor.EquivalentToText"
                           id="_1oge" local="true" varname="sensitivity"/>
            <FetchTextStep client="$(client)"
                           component="SfiControl.sensitivityFB.unit"
                           id="_1ogf" local="true" varname="sensitivityUnit"/>
          </IfSequence>
          <IfSequence id="_1s5C" test="'$(sensitivityUnit)' == 'mK'">
            <SetGlobalStep id="_1s5D" local="true" varname="sensitivity">
              <default>$[$(sensitivity) / 1000]</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1s5E" local="true" varname="sensitivityUnit">
              <default>K</default>
            </SetGlobalStep>
          </IfSequence>
          <IfSequence id="_1ogg" test="'$(sensitivityUnit)' == 'mJy'">
            <SetGlobalStep id="_1ogh" local="true" varname="sensitivity">
              <default>$[$(sensitivity) / 1000]</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1ogi" local="true" varname="sensitivityUnit">
              <default>Jy</default>
            </SetGlobalStep>
          </IfSequence>
          <IfSequence id="_1ogj" test="'$(sensitivityUnit)' == 'uJy'">
            <SetGlobalStep id="_1ogk" local="true" varname="sensitivity">
              <default>$[$(sensitivity) / 1000000]</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1ogl" local="true" varname="sensitivityUnit">
              <default>Jy</default>
            </SetGlobalStep>
          </IfSequence>
          <SetGlobalStep id="_1og9" local="true" varname="result">
            <default>$(sensitivity) $(sensitivityUnit)</default>
          </SetGlobalStep>
          <ReturnStep id="_1ogA" retval="$(result)"/>
        </Procedure>
