            <ComponentWaiter client="$(client)" component="dialogStartupLogin"
                             id="_1lNM" local="true" raise="false"
                             resvarname="reappeared"/>
            <IfSequence id="_1lNN" test="$(reappeared)">
              <FetchTextStep client="$(client)"
                             component="dialogStartupLogin.text" id="_1uPA"
                             local="true" varname="message"/>
              <ProcedureCall id="_1uPB" procedure="qfs.run-log.logError">
                <variable name="message">Login for user '$(userid)' failed with error message:
------------------------------%&lt;------------------------------
$(message)
------------------------------&gt;%------------------------------
</variable>
                <variable name="withScreenshots">true</variable>
              </ProcedureCall>
              <MouseEventStep clicks="1" client="$(client)"
                              component="dialogStartupLogin.buttonCancel"
                              event="MOUSE_MPRC" id="_1uPE" modifiers="16"/>
              <ComponentWaiter client="$(client)" component="dialogWarning"
                               id="_1uPF"/>
              <MouseEventStep clicks="1" client="$(client)"
                              component="WarningBox.buttonOK"
                              event="MOUSE_MPRC" id="_1uPG" modifiers="16"/>
              <ComponentWaiter client="$(client)" component="dialogWarning"
                               id="_1uPH" local="true" raise="false"
                               resvarname="repeated" timeout="5000"/>
              <IfSequence id="_1uPJ" test="$(repeated)">
                <MouseEventStep clicks="1" client="$(client)"
                                component="WarningBox.buttonOK"
                                event="MOUSE_MPRC" id="_1uPI" modifiers="16"/>
              </IfSequence>
              <ProcedureCall disabled="true" id="_1lNO"
                             procedure="qfs.run-log.logError">
                <variable name="message">Login for user '$(userid)' failed.</variable>
                <variable name="withScreenshots">true</variable>
              </ProcedureCall>
              <ElseSequence id="_1lNP" name="Login succeeded">
                <SetGlobalStep id="_1lNK" local="true" varname="result">
                  <default>true</default>
                </SetGlobalStep>
              </ElseSequence>
            </IfSequence>
            <ElseSequence id="_1lNI" name="No login required">
              <SetGlobalStep id="_1lNJ" local="true" varname="result">
                <default>true</default>
              </SetGlobalStep>
            </ElseSequence>
          </IfSequence>
          <ReturnStep id="_1lNH" retval="$(result)"/>
        </Procedure>
