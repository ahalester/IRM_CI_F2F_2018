          <Procedure id="_1oOj" name="checkScienceGoalSummaryView">
            <variable name="type"/>
            <variable name="name"/>
            <comment>Summary view consistency check for the specified science goal.
The values displayed in the specified science goal's summary view are cross-checked with the settings collected from the various input masks.

@param	type	Project type. One of the values (Proposal, Program).
@param	name	Science goal name.

@author	Peter Kratzer
</comment>
            <ProcedureCall disabled="true" id="_1oOl" local="true"
                           procedure="almaot.project.getTabID"
                           retvarname="tab">
              <variable name="type">$(type)</variable>
            </ProcedureCall>
            <ProcedureCall disabled="true" id="_1oOm" local="true"
                           procedure="almaot.project.getTreePathDetail"
                           retvarname="path">
              <variable name="type">$(type)</variable>
            </ProcedureCall>
            <ProcedureCall disabled="true" id="_1oOo"
                           procedure="qfs.swing.tree.selectNode">
              <variable name="id">$(tab).tree</variable>
              <variable name="node">$(path)/$(name)/Field Setup</variable>
              <variable name="separator">%</variable>
            </ProcedureCall>
            <ProcedureCall id="_1qk7"
                           procedure="almaot.project.selectScienceGoalFieldSetup">
              <variable name="type">$(type)</variable>
              <variable name="name">$(name)</variable>
            </ProcedureCall>
            <SetGlobalStep id="_1oPA" local="true" varname="count">
              <default>0</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1oPB" local="true" varname="continue">
              <default>true</default>
            </SetGlobalStep>
            <WhileSequence id="_1oPC"
                           test="$(count) &lt; $(MaxNumSources) and $(continue)">
              <comment>A maximum of 50 sources is permitted in one science goal.</comment>
              <TryStep id="_1oPD" name="Access tab if existent">
                <MouseEventStep clicks="1" client="$(client)"
                                component="spatialFormPanel.tabbed&amp;$(count)"
                                event="MOUSE_MPRC" id="_1oPE" modifiers="16"
                                x="1" y="1"/>
                <CatchSequence exception="IndexNotFoundException" id="_1oPI"
                               maxerror="0">
                  <SetGlobalStep id="_1oPJ" local="true" varname="continue">
                    <default>false</default>
                  </SetGlobalStep>
                </CatchSequence>
              </TryStep>
              <IfSequence id="_1oPN" test="$(continue)">
                <FetchTextStep client="$(client)"
                               component="TargetEditor.sourceName" id="_1oPO"
                               local="true" varname="sourceNameRef"/>
                <ProcedureCall id="_1oPb" local="true"
                               procedure="almaot.project.getScienceGoalTargetType"
                               retvarname="targetTypeRef">
                  <variable name="type">Proposal</variable>
                  <variable name="name">$(name)</variable>
                </ProcedureCall>
                <IfSequence id="_1oPq" test="'$(targetTypeRef)' == 'points'">
                  <CheckBooleanStep checktype="visible" client="$(client)"
                                    component="ScienceGoalFieldSetup.Rectangle.Num12mArrayPointings.text"
                                    disabled="true" id="_1oQ0"/>
                  <FetchTextStep client="$(client)"
                                 component="PointingPatternEditor.Npointings"
                                 id="_1oQ1" local="true"
                                 varname="numPointingsRef"/>
                  <ElseifSequence id="_1oPv"
                                  test="'$(targetTypeRef)' == 'rectangle'">
                    <FetchTextStep client="$(client)"
                                   component="ScienceGoalFieldSetup.Rectangle.Num12mArrayPointings.text"
                                   id="_1oQ2" local="true"
                                   varname="numPointingsRef"/>
                  </ElseifSequence>
                  <ElseSequence id="_1oP+">
                    <ThrowStep id="_1oP-"
                               throwmessage="Unknown target type '$(targetTypeRef)' (field setup)."/>
                  </ElseSequence>
                </IfSequence>
                <FetchTextStep client="$(client)"
                               component="ScienceGoalFieldSetup.SrcCoordRA.text"
                               disabled="true" id="_1oQA" local="true"
                               varname="raRef"/>
                <FetchTextStep client="$(client)"
                               component="ScienceGoalFieldSetup.SrcCoordDec.text"
                               disabled="true" id="_1oQB" local="true"
                               varname="decRef"/>
                <ProcedureCall id="_1ojM" local="true"
                               procedure="almaot.project.getScienceGoalFieldSetupSourceRA"
                               retvarname="raRef">
                  <variable name="type">$(type)</variable>
                  <variable name="name">$(name)</variable>
                </ProcedureCall>
                <ProcedureCall id="_1ojN" local="true"
                               procedure="almaot.project.getScienceGoalFieldSetupSourceDec"
                               retvarname="decRef">
                  <variable name="type">$(type)</variable>
                  <variable name="name">$(name)</variable>
                </ProcedureCall>
                <FetchTextStep client="$(client)"
                               component="VelocityEditor.RedShiftText"
                               id="_1oQo" local="true" varname="redshiftRef"/>
