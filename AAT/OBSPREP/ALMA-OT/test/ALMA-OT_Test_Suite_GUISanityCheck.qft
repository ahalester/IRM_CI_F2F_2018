@author	Peter Kratzer</comment>
        <TryStep id="_1uzH">
          <MouseEventStep clicks="0" client="$(client)" component="mainWindow"
                          event="MOUSE_MOVED" hardevent="true" id="_1uQu"
                          modifiers="0" x="10" y="10"/>
