        <Procedure id="_1lXJ" name="setScienceGoalSpectralSetupContinuum">
          <variable name="type"/>
          <variable name="name">Science Goal</variable>
          <variable name="polarization">DUAL</variable>
          <variable name="receiverBand"/>
          <variable name="resetStdFreq"/>
          <variable name="frequency">100.0</variable>
          <variable name="frequencyUnit">GHz</variable>
          <variable name="calibrator">false</variable>
          <comment>The specified science goal's parameters are set for single continuum.

@param	type	Project type. One of the values (Proposal, Program).
@param	name	Science goal name.
@param	polarization	Polarization. One of the values (XX, DUAL, FULL).
@param	receiverBand	ALMA receiver band. Permitted values: [1, 10].
@param	resetStdFreq	Optional flag (boolean) whether frequency is to be reset to standard value.
@param	frequency	Frequency value.
@param	frequencyUnit	Frequency unit.
@param	calibrator	Flag (boolean) for calibrator science goal.

@author	Peter Kratzer
</comment>
          <SetGlobalStep id="_1u8X" local="true" varname="check">
            <default>false</default>
          </SetGlobalStep>
          <ProcedureCall id="_1qkV"
                         procedure="almaot.project.selectScienceGoalSpectralSetup">
            <variable name="type">$(type)</variable>
            <variable name="name">$(name)</variable>
            <variable name="calibrator">$(calibrator)</variable>
          </ProcedureCall>
          <MouseEventStep clicks="1" client="$(client)"
                          component="SpectralType.radioSingleContinuum"
                          event="MOUSE_MPRC" id="_1lXN" modifiers="16"
                          postdelay="1000" x="1" y="1"/>
          <FetchTextStep client="$(client)"
                         component="FrequencyModel.FeedbackArea" id="_1tv9"
                         local="true" varname="altFreq"/>
          <IfSequence id="_1uy3" test="'$(receiverBand)' &lt;&gt; ''">
            <CheckBooleanStep checktype="enabled" client="$(client)"
                              component="observingFrequencyEditor.standardFrequencyBandJComboBox"
                              id="_1uzD" raise="true"/>
            <ProcedureCall id="_1uzE" procedure="qfs.swing.combobox.setValue">
              <variable name="id">observingFrequencyEditor.standardFrequencyBandJComboBox</variable>
              <variable name="listId">popupInvokerName:_observingFrequencyEditor.standardFrequencyBandJComboBox.list</variable>
              <variable name="value">$(receiverBand) .*</variable>
              <variable name="separator">%</variable>
            </ProcedureCall>
          </IfSequence>
          <IfSequence id="_1uy4" test="'$(resetStdFreq)' &lt;&gt; ''">
            <IfSequence id="_1uy5" test="$(resetStdFreq)">
              <MouseEventStep clicks="1" client="$(client)"
                              component="observingFrequencyEditor.resetToStandardButton"
                              event="MOUSE_MPRC" id="_1uzF" modifiers="16"/>
            </IfSequence>
          </IfSequence>
          <IfSequence id="_1pXn" test="'$(polarization)' &lt;&gt; ''">
            <MouseEventStep clicks="1" client="$(client)"
                            component="SpectralType.PolarizationProduct.radio$(polarization)"
                            event="MOUSE_MPRC" id="_1mKI" modifiers="16" x="1"
                            y="1"/>
          </IfSequence>
          <IfSequence id="_1pXq" test="'$(frequencyUnit)' &lt;&gt; ''">
            <FetchTextStep client="$(client)"
                           component="SingleContinuum.InputFrequency.Unit.combo"
                           id="_1pXs" local="true" varname="oldValue"/>
            <TryStep id="_1uxy">
              <ProcedureCall id="_1mK6"
                             procedure="qfs.swing.combobox.setValue">
                <variable name="id">SingleContinuum.InputFrequency.Unit.combo.button</variable>
                <variable name="listId">popupInvokerClass:_JComboBox3.list</variable>
                <variable name="value">$(frequencyUnit)</variable>
                <variable name="separator">@</variable>
              </ProcedureCall>
              <IfSequence id="_1pXt"
                          test="'$(oldValue)' &lt;&gt; '$(frequencyUnit)'">
                <SetGlobalStep id="_1u8b" local="true" varname="check">
                  <default>true</default>
                </SetGlobalStep>
                <IfSequence disabled="true" id="_1pXu"
                            test="'$(frequency)' &lt;&gt; ''">
                  <ProcedureCall id="_1pXr" procedure="qfs.run-log.logError">
                    <variable name="message">Double frequency setting used to compensate for OT input error.</variable>
                    <variable name="withScreenshots">false</variable>
                  </ProcedureCall>
                  <MouseEventStep clicks="1" client="$(client)"
                                  component="SingleContinuum.InputFrequency.text"
                                  event="MOUSE_MPRC" id="_1pXv" modifiers="16"
                                  x="1" y="1"/>
                  <TextInputStep clear="true" client="$(client)"
                                 component="SingleContinuum.InputFrequency.text"
                                 id="_1pXw">
                    <text>$(frequency)</text>
                  </TextInputStep>
                  <KeyEventStep client="$(client)"
                                component="SingleContinuum.InputFrequency.text"
                                event="KEY_PTR" id="_1pXx" keychar="10"
                                keycode="10" modifiers="0"/>
                </IfSequence>
              </IfSequence>
              <CatchSequence exception="DisabledComponentException" id="_1uxz"
                             maxerror="0">
                <comment>Nothing to be done when explicit frequency input is not available, e.g. when ALMA band is fixed.</comment>
              </CatchSequence>
            </TryStep>
          </IfSequence>
          <IfSequence id="_1pXp" test="'$(frequency)' &lt;&gt; ''">
            <FetchTextStep client="$(client)"
                           component="SingleContinuum.InputFrequency.text"
                           id="_1u8Y" local="true" predelay="1000"
                           varname="prevFreq"/>
            <ProcedureCall id="_1u8W" local="true"
                           procedure="almaot.math.equalDecimals"
                           retvarname="equal">
              <variable name="value1">$(frequency)</variable>
              <variable name="value2">$(prevFreq)</variable>
              <variable name="decimals">-5</variable>
            </ProcedureCall>
            <IfSequence id="_1u8Z" test="not $(equal)">
              <SetGlobalStep id="_1u8a" local="true" varname="check">
                <default>true</default>
              </SetGlobalStep>
            </IfSequence>
            <SetGlobalStep disabled="true" id="_1u8s" local="true"
                           varname="frequency">
              <default>99.999</default>
              <comment>DEBUG !!!!!!!!!!!!!!!!!!!!!!</comment>
            </SetGlobalStep>
            <MouseEventStep clicks="1" client="$(client)"
                            component="SpectralType.radioSingleContinuum"
                            disabled="true" event="MOUSE_MPRC" id="_1u8u"
                            modifiers="16" postdelay="1000" x="1" y="1">
              <comment>DEBUG !!!!!!!!!!!!!!!!!!!!!!</comment>
            </MouseEventStep>
            <MouseEventStep clicks="1" client="$(client)"
                            component="SingleContinuum.radioSkyFrequency"
                            disabled="true" event="MOUSE_MPRC" id="_1u8t"
                            modifiers="16" postdelay="1000" x="1" y="1">
              <comment>DEBUG !!!!!!!!!!!!!!!!!!!!!!</comment>
            </MouseEventStep>
            <MouseEventStep clicks="1" client="$(client)"
                            component="SingleContinuum.InputFrequency.text"
                            event="MOUSE_MPRC" id="_1lXO" modifiers="16"
                            postdelay="1000" x="1" y="1"/>
            <TextInputStep clear="true" client="$(client)"
                           component="SingleContinuum.InputFrequency.text"
                           id="_1lXP" postdelay="1000" single="false">
              <text>$(frequency)</text>
            </TextInputStep>
            <KeyEventStep client="$(client)"
                          component="SingleContinuum.InputFrequency.text"
                          event="KEY_PTR" id="_1lXQ" keychar="10" keycode="10"
                          modifiers="0" postdelay="1000"/>
          </IfSequence>
          <IfSequence id="_1u8c" test="$(check)">
            <FetchTextStep client="$(client)"
                           component="FrequencyModel.FeedbackArea" id="_1u8e"
                           local="true" predelay="2000" varname="altFreq2"/>
            <IfSequence id="_1u8f" test="'$(altFreq)' == '$(altFreq2)'">
              <comment>Temporary fix for ICT-530 to make test suite work again.</comment>
              <ProcedureCall id="_1u8g" procedure="qfs.run-log.logError">
                <variable name="message">Force display update to compensate for error described in ICT-530.</variable>
                <variable name="withScreenshots">false</variable>
              </ProcedureCall>
              <ProcedureCall id="_1u8l"
                             procedure="almaot.project.selectScienceGoal">
                <variable name="type">$(type)</variable>
                <variable name="name">$(name)</variable>
                <variable name="calibrator">$(calibrator)</variable>
              </ProcedureCall>
              <ProcedureCall id="_1u8n"
                             procedure="almaot.project.selectScienceGoalSpectralSetup">
                <variable name="type">$(type)</variable>
                <variable name="name">$(name)</variable>
                <variable name="calibrator">$(calibrator)</variable>
              </ProcedureCall>
              <FetchTextStep client="$(client)"
                             component="FrequencyModel.FeedbackArea"
                             id="_1u8o" local="true" predelay="2000"
                             varname="altFreq2"/>
              <IfSequence id="_1u8p" test="'$(altFreq)' == '$(altFreq2)'">
                <comment>Temporary fix for ICT-530 to make test suite work again.</comment>
                <ProcedureCall id="_1u8q" procedure="qfs.run-log.logError">
                  <variable name="message">Display update not successful.</variable>
                  <variable name="withScreenshots">true</variable>
                </ProcedureCall>
              </IfSequence>
            </IfSequence>
          </IfSequence>
        </Procedure>
