                               procedure="almaot.project.getTitle"
                               retvarname="title">
                  <variable name="type">$(projectType)</variable>
                </ProcedureCall>
                <ServerScriptStep id="_1rqc">
                  <code>import re

title = rc.lookup("title")
teststring = rc.lookup("teststring")

check = re.findall(teststring, title)
result = (len(check) == 0)

rc.setLocal("result", result)
