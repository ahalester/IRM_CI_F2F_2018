        <ReturnStep id="_1l6H" retval="$(mydate) $(mytime)"/>
      </Procedure>
      <Procedure id="_1rQO" name="getUTCTimestamp">
        <variable name="date"/>
        <variable name="offsetDays">1</variable>
        <variable name="time"/>
        <variable name="offsetSeconds"/>
        <comment>A UTC time stamp string is created.

@param	date	Date used for the time stamp (optional).
@param	offsetDays	When no date is specified the current day is used with the specified offset.
@param	time	Time used for the time stamp (optional).
@param	offsetSeconds	When no time is specified the current time is used with the specified offset (seconds).

@return	UTC Time stamp string.

@author	Peter Kratzer
</comment>
        <IfSequence id="_1rQS" test="'$(date)' == ''">
          <ProcedureCall id="_1rQP" local="true" procedure="qfs.utils.getDate"
                         retvarname="date">
            <variable name="format">yyyy-MM-dd</variable>
            <variable name="offset">$(offsetDays)</variable>
          </ProcedureCall>
        </IfSequence>
        <IfSequence id="_1rQT" test="'$(time)' == ''">
          <ProcedureCall id="_1rQQ" local="true" procedure="qfs.utils.getTime"
                         retvarname="time">
            <variable name="format">HH:mm:ss</variable>
            <variable name="offset">$(offsetSeconds)</variable>
          </ProcedureCall>
        </IfSequence>
        <ReturnStep id="_1rQR" retval="$(date)T$(time).000Z"/>
      </Procedure>
