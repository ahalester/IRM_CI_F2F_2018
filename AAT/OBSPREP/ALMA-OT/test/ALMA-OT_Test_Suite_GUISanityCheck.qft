          <Procedure id="_1lo2" name="getNewObservingGroupIndex">
            <variable name="parentNode"/>
            <comment>The index of a new observing group is determined by counting the existing ones.

@param	parentNode	Node to contain the new observing group.

@author	Peter Kratzer
</comment>
            <SetGlobalStep disabled="true" id="_1loM" local="true"
                           varname="parentNode">
              <default>/.*/Science Plan/Special Observing Program/test12unitset.*/test12sb.*</default>
              <comment>DEBUG
</comment>
            </SetGlobalStep>
            <SetGlobalStep id="_1lo5" local="true" varname="result">
              <default>0</default>
            </SetGlobalStep>
            <ProcedureCall id="_1lo3" local="true"
                           procedure="almaot.project.getTabID"
                           retvarname="tab">
              <variable name="type">Program</variable>
            </ProcedureCall>
            <ProcedureCall id="_1lo4" procedure="qfs.swing.tree.selectNode">
              <variable name="id">$(tab).tree</variable>
              <variable name="node">$(parentNode)</variable>
              <variable name="separator">%</variable>
            </ProcedureCall>
            <SetGlobalStep id="_1lo7" local="true" varname="count">
              <default>1</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1lo8" local="true" varname="continue">
              <default>true</default>
            </SetGlobalStep>
            <WhileSequence id="_1loA"
                           test="$(count) &lt; 100 and $(continue)">
              <TryStep id="_1loB" name="Access group node if existent">
                <SetGlobalStep id="_1loJ" local="true" varname="path">
                  <default>$(parentNode)/Group $(count) :.*</default>
                  <comment>
</comment>
                </SetGlobalStep>
                <ProcedureCall disabled="true" id="_1loK"
                               procedure="qfs.swing.tree.selectNode">
                  <variable name="id">$(tab).tree</variable>
                  <variable name="node">$(path)</variable>
                  <variable name="separator">%</variable>
                </ProcedureCall>
                <CheckBooleanStep checktype="visible" client="$(client)"
                                  component="$(tab).tree%$(path)" id="_1loS"
                                  local="true" resvarname="nodeVisible"/>
                <IfSequence id="_1loT" test="not $(nodeVisible)">
                  <SetGlobalStep id="_1loU" local="true" varname="continue">
                    <default>false</default>
                  </SetGlobalStep>
                  <SetGlobalStep id="_1loV" local="true" varname="result">
                    <default>$(count)</default>
                  </SetGlobalStep>
                  <ElseSequence id="_1loZ">
                    <SetGlobalStep id="_1loI" local="true" varname="count">
                      <default>$[$(count) + 1]</default>
                    </SetGlobalStep>
                  </ElseSequence>
                </IfSequence>
                <CatchSequence disabled="true"
                               exception="IndexNotFoundException" id="_1loG"
                               maxerror="0">
                  <SetGlobalStep id="_1loH" local="true" varname="continue">
                    <default>false</default>
                  </SetGlobalStep>
                  <SetGlobalStep id="_1loL" local="true" varname="result">
                    <default>$(count)</default>
                  </SetGlobalStep>
                  <SetGlobalStep id="_1loa" local="true" varname="count">
                    <default>$[$(count) + 1]</default>
                  </SetGlobalStep>
                </CatchSequence>
              </TryStep>
            </WhileSequence>
            <ReturnStep id="_1lo9" retval="$(result)"/>
          </Procedure>
