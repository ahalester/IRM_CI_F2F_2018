                  <variable name="maxDialogs">5</variable>
                </ProcedureCall>
              </CatchSequence>
            </TryStep>
            <ElseSequence id="_1l1R"
                          name="Normal Operation: Check Loaded Project">
              <TryStep id="_1k-V"
