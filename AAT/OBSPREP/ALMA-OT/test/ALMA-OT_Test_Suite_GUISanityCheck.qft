  <Dependency id="_1o2L" name="Test Suite Dependencies">
    <comment>Basic setup for automated ALMA-OT tests (test data base enabled, password for advanced user available, ...).

2016.3</comment>
    <DependencyReference id="_1o2d"
                         reference="almaot.dependencies.Preparations"/>
    <CatchSequence exception="DeadlockTimeoutException" id="_1ucO"
                   maxerror="0">
      <ProcedureCall id="_1ucR" procedure="qfs.run-log.logError">
        <variable name="message">Forcibly terminating unresponsive client program.</variable>
        <variable name="withScreenshots">true</variable>
        <comment>This error message has the important function to provide a last screenshot of the hanging client.</comment>
      </ProcedureCall>
      <ClientStopper client="$(client)" id="_1ucP" postdelay="60000"/>
    </CatchSequence>
  </Dependency>
  <TestCase id="TestCaseVersionInfo" name="Version Info" uid="_1tXF">
    <comment>This test case's only purpose is to provide version information to the test reports (including HTML reports).

@author	Peter Kratzer

2016.3
</comment>
    <IfSequence disabled="true" id="_1vD8" test="'$(SYSTEM_OS)' == 'Linux'"/>
    <ProcedureCall id="_1uNX" procedure="qfs.run-log.logMessage">
      <variable name="message">Operating system: $(SYSTEM_OS)</variable>
      <variable name="dontcompactify">true</variable>
      <variable name="report">true</variable>
    </ProcedureCall>
    <ProcedureCall id="_1uNZ" procedure="qfs.run-log.logMessage">
      <variable name="message">Java version: $(SYSTEM_JAVA_VERSION)</variable>
      <variable name="dontcompactify">true</variable>
      <variable name="report">true</variable>
    </ProcedureCall>
    <ProcedureCall id="_1uNi" procedure="almaot.exec.getJavaVersionFile"/>
    <IfSequence id="_1u9+" test="$(STANDALONE_OT_PKG)">
      <ProcedureCall id="_1u9z" procedure="qfs.run-log.logMessage">
        <variable name="message">Using stand-alone package of the ALMA-OT.</variable>
        <variable name="dontcompactify">true</variable>
        <variable name="report">true</variable>
      </ProcedureCall>
      <ElseSequence id="_1u9-">
        <ProcedureCall id="_1uA0" procedure="qfs.run-log.logMessage">
          <variable name="message">Using ALMA-OT included in the ALMA SW release.</variable>
          <variable name="dontcompactify">true</variable>
          <variable name="report">true</variable>
        </ProcedureCall>
        <ProcedureCall id="_1tXO" procedure="almaot.exec.showACSTag"/>
      </ElseSequence>
    </IfSequence>
    <ProcedureCall disabled="true" id="_1u9v"
                   procedure="almaot.exec.getAlmaOTVersion"/>
    <ProcedureCall id="_1u9x" procedure="qfs.run-log.logMessage">
      <variable name="message">ALMA-OT user version: $(ALMA_USER_VERSION)</variable>
      <variable name="dontcompactify">true</variable>
      <variable name="report">true</variable>
    </ProcedureCall>
    <ProcedureCall id="_1tXH" procedure="almaot.exec.showTestSuiteVersion"/>
