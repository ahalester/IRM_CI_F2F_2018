          <Procedure id="_1p6I" name="checkObsGroupTargetTable">
            <comment>Consistency check for the observing group target table.

@author	Peter Kratzer
</comment>
            <CheckBooleanStep checktype="visible" client="$(client)"
                              component="ObservingGroupTargets.table"
                              id="_1p6L" raise="true"/>
            <ProcedureCall id="_1p6M" local="true"
                           procedure="qfs.swing.table.getRowCount"
                           retvarname="rowcount">
              <variable name="id">ObservingGroupTargets.table</variable>
            </ProcedureCall>
            <SetGlobalStep id="_1p6N" local="true" varname="count">
              <default>0</default>
            </SetGlobalStep>
            <SetGlobalStep id="_1p6O" local="true" varname="continue">
              <default>true</default>
            </SetGlobalStep>
            <WhileSequence id="_1p6P"
                           test="$(count) &lt; $(rowcount) and $(continue)">
              <MouseEventStep clicks="1" client="$(client)"
                              component="ObservingGroupTargets.table.Index&amp;$(count)"
                              event="MOUSE_MPRC" id="_1p6n" modifiers="16"/>
              <ProcedureCall id="_1p6S" local="true"
                             procedure="qfs.swing.table.getValueOfCell"
                             retvarname="purpose">
                <variable name="id">ObservingGroupTargets.table</variable>
                <variable name="column">Purpose</variable>
                <variable name="row">$(count)</variable>
                <variable name="columnSeparator">@</variable>
                <variable name="rowSeparator">&amp;</variable>
              </ProcedureCall>
              <ProcedureCall id="_1p6e" local="true"
                             procedure="almaot.project.test.checkPrecedingAtmCalRequired"
                             retvarname="required">
                <variable name="purpose">$(purpose)</variable>
              </ProcedureCall>
              <IfSequence id="_1p6f" test="$(required)">
                <ProcedureCall id="_1p6v" local="true"
                               procedure="qfs.swing.table.getValueOfCell"
                               retvarname="index">
                  <variable name="id">ObservingGroupTargets.table</variable>
                  <variable name="column">Index</variable>
                  <variable name="row">$(count)</variable>
                  <variable name="columnSeparator">@</variable>
                  <variable name="rowSeparator">&amp;</variable>
                </ProcedureCall>
                <IfSequence id="_1p6g" test="$(count) == 0">
                  <ProcedureCall id="_1p6h" procedure="qfs.run-log.logError">
                    <variable name="message">Entry for purpose '$(purpose)' with index $(index) not preceded by required atmospheric calibration entry.</variable>
                    <variable name="withScreenshots">true</variable>
                  </ProcedureCall>
                  <ElseSequence id="_1p6i">
                    <SetGlobalStep id="_1p6j" local="true" varname="previous">
                      <default>$[$(count) - 1]</default>
                    </SetGlobalStep>
                    <ProcedureCall id="_1p6k" local="true"
                                   procedure="qfs.swing.table.getValueOfCell"
                                   retvarname="lastpurpose">
                      <variable name="id">ObservingGroupTargets.table</variable>
                      <variable name="column">Purpose</variable>
                      <variable name="row">$(previous)</variable>
                      <variable name="columnSeparator">@</variable>
                      <variable name="rowSeparator">&amp;</variable>
                    </ProcedureCall>
                    <IfSequence id="_1p6l"
                                test="'$(lastpurpose)' &lt;&gt; 'AtmCal'">
                      <ProcedureCall id="_1p6m"
                                     procedure="qfs.run-log.logError">
                        <variable name="message">Entry for purpose '$(purpose)' with index $(index) not preceded by required atmospheric calibration entry (previous entry contains '$(lastpurpose)').</variable>
                        <variable name="withScreenshots">true</variable>
                      </ProcedureCall>
                    </IfSequence>
                  </ElseSequence>
                </IfSequence>
              </IfSequence>
              <SetGlobalStep id="_1p6Q" local="true" varname="count">
                <default>$[$(count) + 1]</default>
              </SetGlobalStep>
            </WhileSequence>
          </Procedure>
