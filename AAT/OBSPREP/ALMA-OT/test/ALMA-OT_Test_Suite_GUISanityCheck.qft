      <Procedure id="_1l6E" name="getTimestamp">
        <comment>A time stamp string is created.

@return	Time stamp string.

@author	Peter Kratzer
</comment>
        <ProcedureCall id="_1l6F" local="true" procedure="qfs.utils.getDate"
                       retvarname="mydate">
          <variable name="format">yyyy-MM-dd</variable>
          <variable name="offset"/>
        </ProcedureCall>
        <ProcedureCall id="_1l6G" local="true" procedure="qfs.utils.getTime"
                       retvarname="mytime">
          <variable name="format">HH:mm:ss</variable>
