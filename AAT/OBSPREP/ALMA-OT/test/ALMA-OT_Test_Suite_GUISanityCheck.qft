          <Procedure id="_1rTK" name="getACASuggestion">
            <variable name="name"/>
            <comment>The ACA suggestion for the specified science goal is checked.

@param	name	Science goal name.

@return	ACA suggestion. One of the values (unknown, required, unnecessary, impossible).

@author	Peter Kratzer</comment>
            <SetGlobalStep id="_1rTU" local="true" varname="suggestion">
              <default>unknown</default>
            </SetGlobalStep>
            <ProcedureCall id="_1rTL"
                           procedure="almaot.project.selectScienceGoalCtrlPerf">
              <variable name="type">Proposal</variable>
              <variable name="name">$(name)</variable>
            </ProcedureCall>
            <MouseEventStep clicks="1" client="$(client)"
                            component="SfiControl.ACASuggestion"
                            event="MOUSE_MPRC" id="_1rTM" modifiers="16"/>
            <ComponentWaiter client="$(client)"
                             component="dialogACANecessityEstimator"
                             id="_1rTN"/>
            <CheckStringStep client="$(client)"
                             component="dialogACANecessityEstimator.labelACAUseRecommended"
                             id="_1rTa" local="true" regexp="true"
                             resvarname="match">
              <text>ACA use is recommended .*this observation needs the ACA.*</text>
            </CheckStringStep>
            <IfSequence id="_1rTb" test="$(match)">
              <SetGlobalStep id="_1rTc" local="true" varname="suggestion">
                <default>required</default>
              </SetGlobalStep>
              <ElseSequence id="_1rTd">
                <CheckStringStep client="$(client)"
                                 component="dialogACANecessityEstimator.labelACAUseRecommended"
                                 id="_1rTe" local="true" regexp="true"
                                 resvarname="match">
                  <text>ACA use is not recommended .*this observation does not need the ACA.*</text>
                </CheckStringStep>
                <IfSequence id="_1rTf" test="$(match)">
                  <SetGlobalStep id="_1rTg" local="true" varname="suggestion">
                    <default>unnecessary</default>
                  </SetGlobalStep>
                  <ElseSequence id="_1rTh">
                    <CheckStringStep client="$(client)"
                                     component="dialogACANecessityEstimator.labelACAUseRecommended"
                                     id="_1rTi" local="true" regexp="true"
                                     resvarname="match">
                      <text>It is not possible to use the ACA.*</text>
                    </CheckStringStep>
                    <IfSequence id="_1rTj" test="$(match)">
                      <SetGlobalStep id="_1rTk" local="true"
                                     varname="suggestion">
                        <default>impossible</default>
                      </SetGlobalStep>
                    </IfSequence>
                  </ElseSequence>
                </IfSequence>
              </ElseSequence>
            </IfSequence>
            <MouseEventStep clicks="1" client="$(client)"
                            component="dialogACANecessityEstimator.buttonCancel"
                            event="MOUSE_MPRC" id="_1rTS" modifiers="16"/>
            <ReturnStep id="_1rTT" retval="$(suggestion)"/>
          </Procedure>
