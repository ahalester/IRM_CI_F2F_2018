                         maxerror="0" uid="_1uzI">
            <FetchGeometryStep client="$(client)" component="mainWindow"
                               heightvarname="mainHeight" id="_1uzJ"
                               local="true" widthvarname="mainWidth"
                               xvarname="mainX" yvarname="mainY"/>
            <MouseEventStep clicks="0" client="$(client)"
                            component="mainWindow" event="MOUSE_MOVED"
                            hardevent="true" id="_1uy6" modifiers="0"
                            x="$[$(mainWidth) - 10]"
                            y="$[$(mainHeight) - 10]"/>
          </CatchSequence>
        </TryStep>
      </Procedure>
