                                x="1" y="1"/>
                <ElseSequence id="_1rsS">
                  <MouseEventStep clicks="1" client="$(client)"
                                  component="QuestionBoxConfirm.buttonNo"
                                  disabled="true" event="MOUSE_MPRC"
                                  id="_1rsT" modifiers="16" x="1" y="1"/>
                  <MouseEventStep clicks="1" client="$(client)"
                                  component="QuestionBox.buttonNo"
                                  event="MOUSE_MPRC" id="_1s94" modifiers="16"
                                  x="1" y="1"/>
